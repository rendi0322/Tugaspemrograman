kalimat = "UNIVERSITAS NUSA PUTRA SUKABUMI"

print("point a :",kalimat[17:22].lower(),kalimat[12:16].lower())
print("point b :",kalimat[1:11],kalimat[12]+kalimat[14:16],kalimat[17]+kalimat[19:22],kalimat[23]+kalimat[25:28]+kalimat[29:31])
print("point c :",kalimat[22:31],kalimat[17:22],kalimat[12:16],kalimat[0:11])
print("point d :",kalimat[0]+kalimat[12]+kalimat[17]+kalimat[23])
print("point e :",kalimat[8:11],kalimat[14:16]+kalimat[17:19],kalimat[27:31])